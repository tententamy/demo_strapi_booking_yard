{
  "kind": "collectionType",
  "collectionName": "yards",
  "info": {
    "singularName": "yard",
    "pluralName": "yards",
    "displayName": "Yard"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "descriptipons": {
      "type": "blocks"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "maintain_date": {
      "type": "datetime",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "active"
            },
            false
          ]
        }
      },
      "required": false
    },
    "yard_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::yard-type.yard-type",
      "mappedBy": "yard"
    },
    "yard_booking": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::yard-booking.yard-booking",
      "inversedBy": "yards"
    }
  }
}
